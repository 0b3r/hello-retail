    "retail-stream:deploy:prod": "pushd ./retail-stream/ && sls deploy -s prod && popd",
    "user-info-api:deploy:test": "pushd ./user-info/api/ && sls deploy -s test && popd",
    "user-info-api:deploy:demo": "pushd ./user-info/api/ && sls deploy -s demo && popd",
    "user-info-api:deploy:prod": "pushd ./user-info/api/ && sls deploy -s prod && popd",
    "web:build:test": "pushd ./web/ && sls buildWebApp -s test",
    "web:build:prod": "pushd ./web/ && sls buildWebApp -s prod",
    "web:build:demo": "pushd ./web/ && sls buildWebApp -s demo",
    "web:deploy:test": "pushd ./web/ && sls deploy -s test && popd",
    "web:deploy:prod": "pushd ./web/ && sls deploy -s prod && popd",
    "web:deploy:demo": "pushd ./web/ && sls deploy -s demo && popd",
    "web:dev:hot": "./node_modules/.bin/webpack-dev-server --config ./web/webpack.config.js --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./web/app/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nordstrom/hello-retail.git"
  },
  "keywords": [
    "retail",
