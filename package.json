    "product-creation-api:deploy:test": "pushd ./product-creation/api/ && sls deploy -s test && popd",
    "product-creation-api:deploy:demo": "pushd ./product-creation/api/ && sls deploy -s demo && popd",
    "product-creation-api:deploy:prod": "pushd ./product-creation/api/ && sls deploy -s prod && popd",
    "retail-stream:deploy:test": "pushd ./retail-stream/ && sls deploy -s test && popd",
